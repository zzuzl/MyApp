<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.zlihj.dao.WorkTypeDao">

    <insert id="insert" parameterType="cn.zlihj.domain.WorkType" keyProperty="id" useGeneratedKeys="true">
        insert into work_type(
            name,
            createTime,
            updateTime
        ) values(
            #{name},
            now(),
            now()
        )
    </insert>

    <select id="list" resultType="cn.zlihj.domain.WorkType">
        select * from work_type
    </select>

</mapper>